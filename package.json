{
    "name": "koda-monorepo",
    "private": true,
    "scripts": {
        "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
        "start:backend": "npm run start --prefix backend",
        "start:frontend": "npm run dev --prefix frontend/frontend-app"
    },
    "devDependencies": {
        "concurrently": "^7.0.0"
    }
}